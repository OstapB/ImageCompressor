 var pictures;
 pictures = {
  name: "name: coala;",
  url: " url: http://animaljaws.com/wp-content/uploads/2014/12/koalas.jpg;",
  format: "format: jpg;",
  compressedSize: "compressedSize:200кб;",
  originalSize: "originalSize: 1мб;",
  fullData : function(){
    return this.name + " " + this.url +" " + this.format + " " +  this.compressedSize + " " + this.originalSize;
     }
};
var listPictures = document.body;
var dataPictures = document.createElement("p");
dataPictures.innerHTML += document.write(fullData());
listPictures.insertBefore(dataPictures,listPictures.children[1]);     